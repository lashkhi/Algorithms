int main(int argc, const char * argv[]){
    NSAutoreleasePool *pool = [[NSAutoreleasePool alloc] init];
    int n;
    scanf("%i",&n);
    
    for (int i=1; i<=n; i++) {
        for (int j = 0; j < n-i; j++) {
            printf(" ");
        }
        for (int k=1; k <= i; k++) {
            printf("#");
        }
        printf("\n");
    }
    [pool drain];
    return 0;
}

